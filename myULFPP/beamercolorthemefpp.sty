% Dictates which colours are used
\ProvidesPackage{beamercolorthemefpp}

\mode<presentation>
% Define Colours
\definecolor{fppblue}{RGB}{0,32,68}
\definecolor{glassgrey}{rgb}{220,220,220}
\definecolor{myblue}{RGB}{51,51,102}
\definecolor{lightskyblue}{RGB}{135,206,250}
\definecolor{mywhite}{RGB}{245,245,220}

% Set Colours
\setbeamercolor{title}{parent=fppblue}
\setbeamercolor{title}{fg=myblue}
\setbeamercolor{subtitle}{parent=fppblue}
\setbeamercolor{author}{parent=fppblue}
\setbeamercolor{author}{fg=myblue}
\setbeamercolor{date}{parent=fppblue}
\setbeamercolor{date}{fg=myblue}
\setbeamercolor{institute}{parent=fppblue}

\setbeamercolor{section title}{parent=fppblue}
\setbeamercolor{subsection title}{parent=fppblue}
\setbeamercolor{frametitle}{parent=myblue}
\setbeamercolor{frametitle}{fg=myblue}
\setbeamercolor{background canvas}{parent=fppblue}
\setbeamercolor{structure}{fg=fppblue}

%\setbeamercolor{normal text}{fg=black!97}
\setbeamercolor{normal text}{fg=navyblue,bg=white}
\setbeamercolor{secsubsec}{fg=white,bg=royalblue1}
\setbeamercolor{shadow}{fg=secinhead,bg=shadowbg}
%\setbeamercolor{frametitle}{fg=white,bg=lightskyblue}
\setbeamercolor{math text}{fg=black}

\setbeamercolor{footnote}{fg=black!97}
\setbeamercolor{page number in head/foot}{fg=myblue}

\mode
<all>